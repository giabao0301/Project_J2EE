{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "applemart-api",
      "image": "giabao144/applemart-api:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:mysql://TODO:3306/applemart"
        }
      ]
    }
  ]
}